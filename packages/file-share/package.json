{
  "name": "@won/file-share",
  "private": true,
  "packageManager": "yarn@4.0.2",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "watch": "yarn run watch:ui && yarn run watch:widget",
    "watch:ui": "yarn run build:ui --watch --mode=development",
    "watch:widget": "yarn run build:widget --watch --mode=development",
    "build": "yarn run build:ui && yarn run build:widget",
    "build:ui": "vite build -c vite.config.ui.ts",
    "build:widget": "vite build -c vite.config.widget.ts"
  },
  "dependencies": {
    "@types/node": "^20.11.7",
    "@vitejs/plugin-react": "^4.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.3",
    "tslib": "^2.6.2",
    "vite-plugin-react-rich-svg": "^1.1.0",
    "vite-plugin-singlefile": "^1.0.0"
  },
  "devDependencies": {
    "@figma/plugin-typings": "^1.85.0",
    "@figma/widget-typings": "^1.9.1",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "autoprefixer": "^10.4.17",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "postcss": "^8.4.35",
    "postcss-url": "^10.1.3",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.2.2",
    "vite": "^5.0.8",
    "vite-plugin-generate-file": "^0.1.1"
  }
}
