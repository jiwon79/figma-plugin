{
  "extends": ["../../tsconfig.base.json"],
  "include": ["src/**/*.d.ts", "src/**/*.ts", "src/**/*.tsx"],
  "compilerOptions": {
    "useDefineForClassFields": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "isolatedModules": false,
    "noEmit": true,
    "jsx": "react-jsx"
  }
}
